<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>it.contrader.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoctorManagementServlet</servlet-name>
    <servlet-class>it.contrader.servlets.admin.DoctorManagementServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>it.contrader.servlets.LogoutServlet</servlet-class>
  </servlet>
  <session-config>
    <session-timeout>4000</session-timeout>
  </session-config>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoctorManagementServlet</servlet-name>
    <url-pattern>/DoctorManagementServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UsersManagementServlet</servlet-name>
    <servlet-class>it.contrader.servlet.doctor.UsersManagementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UsersManagementServlet</servlet-name>
    <url-pattern>/UsersManagementServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RobotManagementServlet</display-name>
    <servlet-name>RobotManagementServlet</servlet-name>
    <servlet-class>it.contrader.servlets.admin.RobotManagementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RobotManagementServlet</servlet-name>
    <url-pattern>/RobotManagementServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RobotServlet</display-name>
    <servlet-name>RobotServlet</servlet-name>
    <servlet-class>it.contrader.servlets.robot.RobotServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RobotServlet</servlet-name>
    <url-pattern>/RobotServlet</url-pattern>
  </servlet-mapping>
</web-app>